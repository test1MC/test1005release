<?xml version="1.0" encoding="utf-8"?>
<CatapultSkin
  Version="2"
  SkinType="Component"
  Comment="HTML5 Menu skin"
  ComponentType="Menu"
  EnableResponsiveOutput="True">
  <Stylesheet>
    <Styles>
      <Style
        Name="Menu">
        <Properties>
          <Property
            Name="BackgroundRepeat">repeat</Property>
          <Property
            Name="BorderTop">solid 1px </Property>
          <Property
            Name="BorderRight">solid 1px </Property>
          <Property
            Name="BorderBottom">solid 1px </Property>
          <Property
            Name="BorderLeft">solid 1px </Property>
          <Property
            Name="BorderBottomLeftRadius">3px</Property>
          <Property
            Name="BorderBottomRightRadius">3px</Property>
          <Property
            Name="BorderTopRightRadius">3px</Property>
          <Property
            Name="BorderTopLeftRadius">3px</Property>
          <Property
            Name="Display">block</Property>
          <Property
            Name="Width">80%</Property>
          <Property
            Name="PaddingLeft">5px</Property>
          <Property
            Name="PaddingRight">5px</Property>
          <Property
            Name="PaddingTop">5px</Property>
          <Property
            Name="PaddingBottom">5px</Property>
          <Property
            Name="MarginLeft">60px</Property>
          <Property
            Name="Color">#000000</Property>
          <Property
            Name="FontSize">1em</Property>
          <Property
            Name="BackgroundColor">#ffffff</Property>
        </Properties>
      </Style>
      <Style
        Name="Menu Item">
        <Properties>
          <Property
            Name="PaddingTop">4px</Property>
          <Property
            Name="PaddingBottom">4px</Property>
          <Property
            Name="BackgroundColor">transparent</Property>
          <Property
            Name="MarginBottom">0px</Property>
          <Property
            Name="MarginTop">0px</Property>
          <Property
            Name="MarginRight">0px</Property>
          <Property
            Name="MarginLeft">0px</Property>
          <Property
            Name="Color">#696969</Property>
        </Properties>
        <Classes>
          <StyleClass
            Name="1st Level">
            <Properties>
              <Property
                Name="PaddingBottom">6px</Property>
              <Property
                Name="PaddingTop">6px</Property>
              <Property
                Name="Color">#000000</Property>
              <Property
                Name="FontWeight">normal</Property>
            </Properties>
          </StyleClass>
          <StyleClass
            Name="2nd Level">
            <Properties>
              <Property
                Name="PaddingTop">2px</Property>
              <Property
                Name="PaddingBottom">2px</Property>
              <Property
                Name="Color">#a9a9a9</Property>
              <Property
                Name="FontWeight">normal</Property>
            </Properties>
          </StyleClass>
          <StyleClass
            Name="3rd Level">
            <Properties>
              <Property
                Name="PaddingTop">2px</Property>
              <Property
                Name="PaddingBottom">2px</Property>
            </Properties>
          </StyleClass>
          <StyleClass
            Name="4th Level">
            <Properties>
              <Property
                Name="PaddingBottom">2px</Property>
              <Property
                Name="PaddingTop">2px</Property>
            </Properties>
          </StyleClass>
          <StyleClass
            Name="Selected">
            <Properties>
              <Property
                Name="BackgroundColor">#67A3C7</Property>
            </Properties>
          </StyleClass>
        </Classes>
      </Style>
      <Style
        Name="Menu Item (hover)">
        <Properties>
          <Property
            Name="BackgroundImage">none</Property>
          <Property
            Name="FontWeight">normal</Property>
          <Property
            Name="BackgroundColor">#67a3c7</Property>
          <Property
            Name="Color">#ffffff</Property>
        </Properties>
        <Classes>
          <StyleClass
            Name="1st Level">
            <Properties>
              <Property
                Name="BackgroundImage">none</Property>
              <Property
                Name="Color">#ffffff</Property>
              <Property
                Name="BackgroundColor">#67a3c7</Property>
            </Properties>
          </StyleClass>
          <StyleClass
            Name="2nd Level">
            <Properties>
              <Property
                Name="Color">#ffffff</Property>
              <Property
                Name="BackgroundColor">#67a3c7</Property>
            </Properties>
          </StyleClass>
          <StyleClass
            Name="4th Level">
            <Properties>
              <Property
                Name="BackgroundColor">#67a3c7</Property>
            </Properties>
          </StyleClass>
        </Classes>
      </Style>
      <Medium
        Name="Mobile">
        <Style
          Name="Menu">
          <Properties>
            <Property
              Name="Display">none</Property>
          </Properties>
        </Style>
      </Medium>
    </Styles>
    <Resources>
      <Resource
        Name="avi-50.png"></Resource>
    </Resources>
  </Stylesheet>
  <Strings />
</CatapultSkin>