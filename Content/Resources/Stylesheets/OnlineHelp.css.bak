	}

	h1
	{
		background-color: transparent;
		border-bottom: none;
		margin-bottom: 20px;
		padding-left: 0px;
		padding-top: 0px;
		font-family: COINS;
		font-weight: bold;
		font-style: normal;
		font-size: 20pt;
		line-height: normal;
		color: #53555a;
		mc-leader-indent: 0.5in;
		mc-leader-offset: 0.5in;
		mc-multiline-indent: 0.5in;
		margin-left: 0.5in;
